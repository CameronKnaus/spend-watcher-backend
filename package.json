  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-jwt": "^6.0.0",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.12.3",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ],
      "indent": [
        "error",
        4
      ],
      "consistent-return": [
        "off"
      ],
      "no-param-reassign": [
        "off"
      ],
      "prefer-promise-reject-errors": [
        "off"
      ],
      "camelcase": "off",
      "max-len": "off",
      "no-plusplus": "off",
      "no-console": "off"
    }
  }
}